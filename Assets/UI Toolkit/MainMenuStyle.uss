    color: rgb(255, 255, 255);
    align-self: auto;
    -unity-font-style: normal;
    -unity-font-definition: url("project://database/Assets/TypeOut/Fonts/HACKED_Title.ttf?fileID=12800000&guid=0d321ad12fe3d4391b506737af0972d1&type=3#HACKED_Title");
