    border-bottom-right-radius: 25px;
    border-bottom-left-radius: 25px;
    -unity-font-style: normal;
    -unity-font-definition: url("project://database/Assets/Fonts/LuckiestGuy-Regular.ttf?fileID=12800000&guid=042ca966657e9e141b5dacf89c3e5da0&type=3#LuckiestGuy-Regular");
